#This install driver installs the working version of dguint located at
#/users/raf/source/dguint in to the working version of
#guint at /users/raf/source/guint
#all files to be transferred must be in readable versions in the requisite
#directories.
pushd /users/raf/source/guint
foreach file  (gp*)
   /bin/rm  /users/raf/source/guint/$file
   sed -e sx/users/raf/source/dguintx/users/raf/source/guintxg $file > /users/raf/source/guint/$file
end
chmod 755 /users/raf/source/guint/*
cd /users/raf/source/dguint/edtsum
foreach file  (gp*)
   /bin/rm  /users/raf/source/guint/edtsum/$file
   sed -e sx/users/raf/source/dguintx/users/raf/source/guintxg $file > /users/raf/source/guint/edtsum/$file
