#This install driver installs the working version of guint located at
#/users/raf/source/guint in to the development version of
#guint at /users/raf/source/nguint
#all files to be transferred must be in readable versions in the requisite
#directories.
pushd /users/raf/source/guint
foreach file  (gp*)
   /bin/rm  /users/raf/source/nguint/$file
   sed -e sx/users/raf/source/guintx/users/raf/source/nguintxg $file > /users/raf/source/nguint/$file
end
chmod 755 /users/raf/source/nguint/*
cd /users/raf/source/guint/edtsum
foreach file  (gp*)
   /bin/rm  /users/raf/source/nguint/edtsum/$file
   sed -e sx/users/raf/source/guintx/users/raf/source/nguintxg $file > /users/raf/source/nguint/edtsum/$file
end
